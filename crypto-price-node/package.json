{
  "name": "crypto-price-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "server": "babel-node ./src/bin/www",
    "dev": "nodemon --exec NODE_ENV=development npm run server",
